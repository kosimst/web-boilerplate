{
  "name": "kosimst-web-boilerplate",
  "version": "1.0.0",
  "contributors": [
    "The Polymer Authors",
    "Simon Steindl"
  ],
  "license": "BSD-3-Clause",
  "repository": "kosimst/web-boilerplate",
  "scripts": {
    "start": "gulp serve",
    "build": "tsc && polymer build --auto-base-path && gulp prpl-server",
    "build:static": "tsc && polymer build",
    "serve": "prpl-server --root server/build",
    "serve:static": "polymer serve --port 5000 build/es5-bundled",
    "test": "npm run test:unit && npm run test:integration",
    "test:integration": "tsc && mocha test/integration --timeout=10000",
    "test:unit": "tsc && polymer test --module-resolution=node --npm",
    "test:sauce": "tsc && polymer test --module-resolution=node --npm -s 'windows 10/microsoftedge@17' -s 'macos 10.13/safari@11'",
    "test:regenerate_screenshots": "tsc && mocha test/integration/screenshots-baseline/regenerate.js --timeout=15000"
  },
  "dependencies": {
    "@material/drawer": "^3.1.1",
    "@material/list": "^3.1.0",
    "@material/mwc-drawer": "^0.6.0",
    "@material/mwc-top-app-bar": "^0.6.0",
    "@material/top-app-bar": "^3.1.0",
    "@polymer/app-layout": "^3.1.0",
    "@polymer/iron-icon": "^3.0.1",
    "@polymer/iron-icons": "^3.0.1",
    "@polymer/polymer": "^3.1.0",
    "@vaadin/vaadin-app-layout": "^2.0.2",
    "@vaadin/vaadin-button": "^2.2.1",
    "@vaadin/vaadin-dialog": "^2.2.1",
    "@vaadin/vaadin-form-layout": "^2.1.5",
    "@vaadin/vaadin-login": "^1.0.1",
    "@vaadin/vaadin-tabs": "^3.0.4",
    "@vaadin/vaadin-text-field": "^2.4.10",
    "@webcomponents/webcomponentsjs": "^2.2.4",
    "firebase": "^6.3.5",
    "lit-element": "^2.0.1",
    "lit-html": "^1.1.2",
    "pwa-helpers": "^0.9.0",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@polymer/test-fixture": "^4.0.2",
    "axe-core": "^3.0.0",
    "chai": "^4.1.2",
    "del": "^3.0.0",
    "gulp": "^4.0.0",
    "gulp-rename": "^1.3.0",
    "gulp-replace": "^1.0.0",
    "mocha": "^5.2.0",
    "pixelmatch": "^4.0.2",
    "polymer-cli": "^1.9.3",
    "polyserve": "^0.27.0",
    "prpl-server": "^1.4.0",
    "puppeteer": "^1.5.0",
    "typescript": "^3.1.3",
    "wct-mocha": "^1.0.0"
  }
}
